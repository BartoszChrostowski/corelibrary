<Project>
  <!-- Common libraries -->
  <PropertyGroup>
    <!-- Self version, generated automatically -->
    <CoreLibVersion>GENERATED_DURING_BUILD</CoreLibVersion>

    <AspNetCoreVersion>2.1.0-rc1-final</AspNetCoreVersion>
    <!-- .NET Core/ASP.NET Core extensions, mostly Microsoft.AspNetCore.* (other than Mvc) and Microsoft.Extensions.* packages -->
    <ExtensionsVersion>2.1.0-rc1-final</ExtensionsVersion>
    <JsonNetVersion>11.0.2</JsonNetVersion>
    <AutofacVersion>4.8.1</AutofacVersion>
    <AutoMapperVersion>6.2.2</AutoMapperVersion>
    <SerilogVersion>2.6.0</SerilogVersion>

    <MicrosoftExtensionsConfigurationVersion>2.1.0-rc1-final</MicrosoftExtensionsConfigurationVersion>
    <MicrosoftExtensionsConfigurationJsonVersion>2.1.0-rc1-final</MicrosoftExtensionsConfigurationJsonVersion>

    <!-- System.* libraries -->
    <NetCoreLibVersion>4.4.0</NetCoreLibVersion>
    <EFCoreVersion>2.1.0-rc1-final</EFCoreVersion>

    <!-- Libs used in one place -->
    <AutofacExtensionsDependencyInjectionVersion>4.2.0</AutofacExtensionsDependencyInjectionVersion>
    <FluentValidationVersion>7.5.2</FluentValidationVersion>
    <PollyVersion>6.0.1</PollyVersion>

    <SerilogExtensionsLoggingVersion>2.0.2</SerilogExtensionsLoggingVersion>
    <SerilogSettingsConfigurationVersion>2.6.1</SerilogSettingsConfigurationVersion>
    <SerilogSinksLiterateVersion>3.0.0</SerilogSinksLiterateVersion>
    <SerilogSinksRollingFileVersion>3.3.0</SerilogSinksRollingFileVersion>

    <!-- Tests -->
    <XUnitVersion>2.3.1</XUnitVersion>
    <XUnitAnalyzersVersion>0.9.0</XUnitAnalyzersVersion>
    <MicrosoftNETTestSdkVersion>15.7.2</MicrosoftNETTestSdkVersion>
    <NSubstituteVersion>3.1.0</NSubstituteVersion>

    <!-- Razor -->
    <MicrosoftCSharpVersion>4.4.1</MicrosoftCSharpVersion>
    <MicrosoftCodeAnalysisCSharpVersion>2.8.0</MicrosoftCodeAnalysisCSharpVersion>
    <SystemRuntimeLoaderVersion>4.3.0</SystemRuntimeLoaderVersion>
    <SystemDynamicRuntimeVersion>4.3.0</SystemDynamicRuntimeVersion>

    <!-- IdentityServer & related -->
    <IdentityServer4Version>2.2.0</IdentityServer4Version>
    <MicrosoftAzureKeyVaultVersion>2.3.2</MicrosoftAzureKeyVaultVersion>
    <MicrosoftIdentityModelClientsActiveDirectoryVersion>3.19.4</MicrosoftIdentityModelClientsActiveDirectoryVersion>
    <IdentityModelVersion>3.6.1</IdentityModelVersion>

    <DapperVersion>1.50.4</DapperVersion>
  </PropertyGroup>
</Project>
