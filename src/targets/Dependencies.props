<Project>
  <!-- Common libraries -->
  <PropertyGroup>
    <!-- Self version, generated automatically -->
    <CoreLibVersion>GENERATED_DURING_BUILD</CoreLibVersion>

    <AspNetCoreVersion>2.1.1</AspNetCoreVersion>
    <!-- .NET Core/ASP.NET Core extensions, mostly Microsoft.AspNetCore.* (other than Mvc) and Microsoft.Extensions.* packages -->
    <ExtensionsVersion>2.1.1</ExtensionsVersion>
    <JsonNetVersion>11.0.2</JsonNetVersion>
    <AutofacVersion>4.8.1</AutofacVersion>
    <AutoMapperVersion>7.0.1</AutoMapperVersion>
    <SerilogVersion>2.7.1</SerilogVersion>

    <MicrosoftAspNetCoreServerKestrelVersion>2.1.2</MicrosoftAspNetCoreServerKestrelVersion>

    <MicrosoftExtensionsConfigurationVersion>2.1.1</MicrosoftExtensionsConfigurationVersion>
    <MicrosoftExtensionsConfigurationJsonVersion>2.1.1</MicrosoftExtensionsConfigurationJsonVersion>

    <!-- System.* libraries -->
    <SystemThreadingTasksExtensionsVersion>4.5.1</SystemThreadingTasksExtensionsVersion>
    <SystemValueTupleVersion>4.5.0</SystemValueTupleVersion>
    <NetCoreLibVersion>4.4.0</NetCoreLibVersion>
    <EFCoreVersion>2.1.1</EFCoreVersion>

    <!-- Libs used in one place -->
    <AutofacExtensionsDependencyInjectionVersion>4.2.2</AutofacExtensionsDependencyInjectionVersion>
    <FluentValidationVersion>7.6.104</FluentValidationVersion>
    <PollyVersion>6.1.0</PollyVersion>

    <SerilogExtensionsLoggingVersion>2.0.2</SerilogExtensionsLoggingVersion>
    <SerilogSettingsConfigurationVersion>2.6.1</SerilogSettingsConfigurationVersion>
    <SerilogSinksLiterateVersion>3.0.0</SerilogSinksLiterateVersion>
    <SerilogSinksRollingFileVersion>3.3.0</SerilogSinksRollingFileVersion>

    <!-- Tests -->
    <XUnitVersion>2.4.0</XUnitVersion>
    <XUnitAnalyzersVersion>0.10.0</XUnitAnalyzersVersion>
    <MicrosoftNETTestSdkVersion>15.8.0</MicrosoftNETTestSdkVersion>
    <NSubstituteVersion>3.1.0</NSubstituteVersion>

    <!-- Razor -->
    <MicrosoftCSharpVersion>4.5.0</MicrosoftCSharpVersion>
    <MicrosoftCodeAnalysisCSharpVersion>2.8.2</MicrosoftCodeAnalysisCSharpVersion>
    <SystemRuntimeLoaderVersion>4.3.0</SystemRuntimeLoaderVersion>
    <SystemDynamicRuntimeVersion>4.3.0</SystemDynamicRuntimeVersion>

    <!-- IdentityServer & related -->
    <IdentityServer4Version>2.2.0</IdentityServer4Version>
    <MicrosoftAzureKeyVaultVersion>3.0.0</MicrosoftAzureKeyVaultVersion>
    <MicrosoftIdentityModelClientsActiveDirectoryVersion>3.19.8</MicrosoftIdentityModelClientsActiveDirectoryVersion>
    <IdentityModelVersion>3.8.0</IdentityModelVersion>

    <DapperVersion>1.50.5</DapperVersion>

    <HangfireVersion>1.7.0-beta1</HangfireVersion>
    <HangfireAutofacVersion>2.3.1</HangfireAutofacVersion>

    <!-- Tools version -->
    <GlobVersion>0.4.0</GlobVersion>
    <MicrosoftCodeAnalysisVersion>2.8.2</MicrosoftCodeAnalysisVersion>
    <MicrosoftExtensionsConfigurationBinderVersion>2.1.1</MicrosoftExtensionsConfigurationBinderVersion>
    <MicrosoftExtensionsConfigurationCommandLineVersion>2.1.1</MicrosoftExtensionsConfigurationCommandLineVersion>
  </PropertyGroup>
</Project>
